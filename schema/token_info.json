			"explorer2": {
				"type": "string",
				"format": "uri"
			},								
			"github": {
				"type": "string",
				"format": "uri"
			},
			"reddit": {
				"type": "string",
				"format": "uri"
			},
			"twitter": {
				"type": "string",
				"format": "uri"
			},
			"facebook": {
				"type": "string",
				"format": "uri"
			},
			"telegram": {
				"type": "string",
				"format": "uri"
			},
			"instagram": {
				"type": "string",
				"format": "uri"
			},
			"medium": {
				"type": "string",
				"format": "uri"
			},
			"youtube": {
				"type": "string",
				"format": "uri"
			},
			"linkedin": {
				"type": "string",
				"format": "uri"
