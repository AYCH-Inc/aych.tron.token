{
	"description": "TronWallet Token Info Schema",
	"schema": {
		"$schema": "http://json-schema.org/draft-07/schema#",
		"type": "object",
		"properties": {
			"token": {
				"type": "string"
			},				
			"decimals": {
				"type": "string"
			},				
			"owner_address": {
				"type": "string"
			},				
			"description": {
				"type": "string"
			},				
			"utility": {
				"type": "string"
			},				
			"max_supply": {
				"type": "string"
			},				
			"circulating_supply": {
				"type": "string"
			},			
			"circulating_url": {
				"type": "string",
