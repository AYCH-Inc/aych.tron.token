			},	
			"consensus": {
				"type": "string"
			},	
			"website": {
				"type": "string",
				"format": "uri"
			},	
			"whitepaper": {
				"type": "string",
				"format": "uri"
			},			
			"issue_date": {
				"type": "string"
			},	
			"issue_price": {
				"type": "string"
			},																								

			"explorer1": {
				"type": "string",
